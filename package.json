{
  "name": "@momofy/sdk",
  "type": "module",
  "version": "1.0.5",
  "description": "Momofy SDK for Node Js Applications",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js",
    "types": "./types/*.d.ts"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "vitest",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "source": "src/index.ts",
    "main": "dist/index.js",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MomofyApp/momofy-nodejs-sdk.git"
  },
  "author": "roqkabel <roqkabel@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "keywords": [
    "momofy",
    "momo",
    "mobile-money",
    "mtn",
    "tigo",
    "airtel-tigo",
    "vodafone"
  ],
  "files": [
    "dist"
  ]
}
